{
  "name": "google-photo-app",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "axios": "^0.25.0",
    "bootstrap-icons": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "postcss": "^8.5.6",
    "postcss-safe-parser": "^7.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.6",
    "react-scripts": "^4.0.1",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.1",
    "uuid": "^8.3.1",
    "websocket": "^1.0.34",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.4",
    "nodemon": "^2.0.6",
    "prettier": "^2.1.2"
  },
  "scripts": {
    "start": "concurrently \"npm:start:server\" \"npm:start:client\"",
    "start:server": "nodemon server/index.js",
    "start:client": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "doc/*",
      "node_modules/*",
      "src/*"
    ],
    "delay": "1500"
  },
    "engines": {
      "node": "16.x"
    }
}
